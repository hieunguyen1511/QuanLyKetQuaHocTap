
@{
    ViewBag.Title = "Thời khoá biểu";
    Layout = "~/Views/SinhVien/_LayoutSinhVien.cshtml";
}

<link href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.0/dist/css/bootstrap.min.css" rel="stylesheet">
<link rel="stylesheet" href="~/Content/Template_Info/main.css" />
@{
    int hocki = (int)ViewBag.HocKi;
    int nam = (int)ViewBag.Nam;
    DateTime date = (DateTime)ViewBag.Date;
    Html.RenderAction("TKB_Partial", new { day = date.Day, month = date.Month, year = date.Year, hocki = hocki, nam = nam });
}


<script src="https://code.jquery.com/jquery-1.10.2.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@4.5.0/dist/js/bootstrap.bundle.min.js"></script>
<script type="text/javascript"></script>
